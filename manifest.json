{
  "manifest_version": 2,
  "name": "GitFit",
  "version": "1.0",

  "description": "Stego the Tabagotchi",

  "background": {
    "scripts": ["src/main/stego.js"],
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],

  "chrome_url_overrides": {
    "newtab": "src/main/stego.html"
  }

}

